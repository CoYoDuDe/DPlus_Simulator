#!/bin/sh
set -eu
if set -o pipefail >/dev/null 2>&1; then
  :
fi

SERVICE_NAME="com.coyodude.dplussim"
PIPE="/data/SetupHelper/HelperResources/serviceLogPipe"

if [ -x "${PIPE}" ]; then
  exec "${PIPE}" "${SERVICE_NAME}"
fi

if command -v svlogd >/dev/null 2>&1; then
  exec svlogd -tt .
fi

exec logger -t "${SERVICE_NAME}" -p daemon.info
